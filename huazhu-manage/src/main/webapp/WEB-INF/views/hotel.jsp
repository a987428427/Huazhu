			$(function () {
	        var body = $('body')
	    
	        function MarkerOverlay(center, length) {
	          this._center = center;
	          this._length = length;
	        }
	    
	        MarkerOverlay.prototype = new BMap.Overlay();
	        MarkerOverlay.prototype.initialize = function (map) {
	          this._map = map;
	          var pos = document.createElement("i")
	          pos.className = "pos icon icondingweihongse blue pull-left"
	    
	          var span0 = document.createElement("span")
	          span0.className = "pull-left span0"
	          span0.innerText = 1
	    
	          var span = document.createElement("span")
	          span.innerHTML = hotelName
	          span.className = "black three-dot pull-left span1"
	    
	          var span2 = document.createElement("span")
	          span2.innerHTML = (hotelPrice < 0 || stock === 0) ? '(已订满)' : ("¥" + hotelPrice);
	          span2.className = "pull-left span2"
	    
	          var fragment = document.createDocumentFragment();
	          var div = document.createElement("div");
	          div.style.position = "absolute";
	          div.style.width = this._length + "px";
	          fragment.appendChild(span);
	          fragment.appendChild(span2);
	    
	          var spanSuper = document.createElement("span")
	          spanSuper.className = "clearfix span-super"
	          spanSuper.appendChild(fragment);
	          div.appendChild(pos);
	          div.appendChild(span0);
	          div.appendChild(spanSuper);
	    
	          map.getPanes().markerPane.appendChild(div);
	          this._div = div;
	          return div;
	        }
	    
	        var createMap = function (opts) {
	    
	          var type = opts.type || 'small';
	          var eleid = opts.eleid;
	          var longitude = opts.longitude;
	          var latitude = opts.latitude;
	          var dragging = opts.dragging || false;
	          var scrollZoom = opts.scrollZoom || false;
	    
	          if (!(eleid && longitude && latitude)) return;
	    
	          var map = new BMap.Map(eleid);
	          var point = new BMap.Point(longitude, latitude);
	          var view = map.getViewport([point]);
	          var mapZoom = view.zoom;
	          map.centerAndZoom(point, mapZoom);
	          MarkerOverlay.prototype.draw = function () {
	            var position = this._map.pointToOverlayPixel(this._center);
	            this._div.style.left = position.x - (type === 'small' ? 14 : 22) + "px";
	            this._div.style.top = position.y - (type === 'small' ? 30 : 52) + "px";
	          }
	          var cm = new MarkerOverlay(point, 230)
	          map.addOverlay(cm)
	          if (dragging) {
	            map.disableDragging()
	          }
	          if (scrollZoom) {
	            map.enableScrollWheelZoom(true);
	          }
	        }
	        createMap({
	          eleid: 'map',
	          longitude: longitude,
	          latitude: latitude,
	          type: 'small'
	        })
	        document.addEventListener('click', (e) => {
	          var target = $(e.target);
	          if (target.hasClass('map')) {
	            var alertMapEleWrap = $('<div id="alert-map-wrap" class="alert-map-wrap">' +
	              '<div class="alert-map-content"> <div class="close icon iconguanbi"></div> </div>' +
	              '</div>')
	            var alertMapEle = $('<div id="alert-map" class="alert-map">')
	            alertMapEleWrap.find('.alert-map-content').append(alertMapEle)
	            body.append(alertMapEleWrap)
	            body.css({height: '100%', overflow: 'hidden'})
	            createMap({
	              eleid: 'alert-map',
	              longitude: longitude,
	              latitude: latitude,
	              type: 'large',
	              scrollZoom: true
	            })
	          }
	          if (target.hasClass('close') || target.hasClass('alert-map-wrap')) {
	            var alertMapEleWrap = $('#alert-map-wrap')
	            alertMapEleWrap.remove()
	            body.css({height: 'auto', overflow: 'visible'})
	          }
	        });
	      })
